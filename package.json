    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.4",
    "@angular/cli": "~8.3.4",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@types/node": "~8.9.4",
    "angular-tslint-rules": "^1.20.1",
    "codelyzer": "^5.1.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
